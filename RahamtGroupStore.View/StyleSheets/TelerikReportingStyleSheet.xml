<?xml version="1.0" encoding="utf-8"?>
<StyleSheet xmlns="http://schemas.telerik.com/reporting/2012/2">
  <StyleRule>
    <Style>
      <Padding Left="2pt" Right="2pt" />
    </Style>
    <Selectors>
      <TypeSelector Type="TextItemBase" />
      <TypeSelector Type="HtmlTextBox" />
    </Selectors>
  </StyleRule>
  <StyleRule>
    <Style BackgroundColor="White" TextAlign="Center">
      <BorderStyle Bottom="Solid" />
      <Font Size="15pt" />
    </Style>
    <Selectors>
      <StyleSelector Type="ReportItemBase" StyleName="HeaderStyle" />
    </Selectors>
  </StyleRule>
  <StyleRule>
    <Style BackgroundColor="224, 224, 224">
      <BorderStyle Default="Solid" Top="None" Right="None" />
      <Padding Left="4pt" Right="2pt" Top="1pt" />
    </Style>
    <Selectors>
      <StyleSelector Type="ReportItemBase" StyleName="GroupHeaderStyle" />
    </Selectors>
  </StyleRule>
  <StyleRule>
    <Style>
      <BorderStyle Default="None" Top="None" Bottom="None" Left="None" Right="None" />
      <Padding Left="4pt" Right="2pt" Top="1pt" />
    </Style>
    <Selectors>
      <StyleSelector Type="ReportItemBase" StyleName="DetailsStyle" />
    </Selectors>
  </StyleRule>
  <StyleRule>
    <Style>
      <Font Size="12pt" Bold="True" />
    </Style>
    <Selectors>
      <StyleSelector Type="ReportItemBase" StyleName="GroupingStyle" />
    </Selectors>
  </StyleRule>
</StyleSheet>